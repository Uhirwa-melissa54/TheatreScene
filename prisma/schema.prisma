

generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Admin{
  id        Int      @id @default(autoincrement())
  firstName String
  lastName String 
  email String @unique
  username String @unique
  password String
  nationalId String @unique
  level String
  createdAt DateTime @default(now())

}